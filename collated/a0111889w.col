     */
    private void formatTextLabel(Text Label, String color) {
        assert Label != null && color != null && !color.trim().isEmpty();
        Label.setFont(Font.font("Lucida Grande", FontWeight.BOLD, 12));
        Label.setFill(Color.web(color));
